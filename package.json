{
  "name": "my-meal-app",
  "version": "1.0.0",
  "description": "Book A Meal App",
  "main": "App.js",
  "scripts": {
    "start": "babel-node server/src/app.js",
    "test": "nyc --reporter=html --reporter=text mocha --timeout 50000 --exit --require @babel/register server/src/test/test.js --exit",
    "start:dev": "nodemon --watch server --exec babel-node server/src/app.js",
    "coveralls-coverage": "coveralls < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidfrank96/My-Meal-App.git"
  },
  "author": "Frank David",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidfrank96/My-Meal-App/issues"
  },
  "homepage": "https://github.com/davidfrank96/My-Meal-App#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "supertest": "^3.4.2"
  }
}
